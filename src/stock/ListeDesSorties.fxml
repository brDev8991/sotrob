<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="stock.ListeDesSorties">
   <children>
      <BorderPane AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <padding>
            <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
         </padding>
         <center>
            <VBox alignment="CENTER" BorderPane.alignment="CENTER">
               <children>
                  <BorderPane>
                     <right>
                        <TextField fx:id="tf_rechercher" alignment="CENTER" promptText="Rechercher" BorderPane.alignment="CENTER" />
                     </right>
                     <left>
                        <Label text="Liste Des Sorties" BorderPane.alignment="CENTER">
                           <font>
                              <Font name="Lucida Console" size="20.0" />
                           </font>
                        </Label>
                     </left>
                     <VBox.margin>
                        <Insets bottom="20.0" />
                     </VBox.margin>
                  </BorderPane>
                  <TableView fx:id="tableSorties">
                    <columns>
                      <TableColumn fx:id="col1" maxWidth="500.0" minWidth="100.0" prefWidth="-1.0" text="DATE" />
                      <TableColumn fx:id="col2" maxWidth="500.0" minWidth="50.0" prefWidth="-1.0" text="N.B.S" />
                        <TableColumn fx:id="col3" maxWidth="500.0" minWidth="150.0" prefWidth="-1.0" text="DESIGNATION" />
                        <TableColumn fx:id="col4" maxWidth="500.0" minWidth="150.0" prefWidth="-1.0" text="QTE" />
                        <TableColumn fx:id="col5" maxWidth="500.0" minWidth="50.0" prefWidth="-1.0" text="P.U" />
                        <TableColumn fx:id="col6" maxWidth="500.0" minWidth="100.0" prefWidth="-1.0" text="DESTINATION" />
                        <TableColumn fx:id="col7" maxWidth="500.0" minWidth="100.0" prefWidth="-1.0" text="IMPUTATION" />
                        <TableColumn fx:id="col8" maxWidth="500.0" minWidth="200.0" prefWidth="-1.0" text="CESSION" />
                    </columns>
                  </TableView>
               </children>
            </VBox>
         </center>
      </BorderPane>
   </children>
</AnchorPane>
